#paste in a.txt


nano b.py


	ascii_string = ""
	with open('a.txt', 'r') as file:
	    for line in file:
	        integer = int(line.strip())
	        character = chr(integer)
	        ascii_string += character

	print("ASCII string:", ascii_string)


save && Exit


python b.py


